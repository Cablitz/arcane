{
    "name": "chainsaw",
    "emitLegacyScripts": false,
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "$path": "./assets/ReplicatedStorage",
            "client": {
                "$path": "dist/client"
            },
            "common": {
                "$path": "dist/common"
            },
            "network": {
                "$path": ".blink/client.luau"
            },
            "packages": {
                "$className": "Folder",
                "$path": "./packages"
            }
        },
        "ServerScriptService": {
            "server": {
                "$path": "dist/server"
            },
            "network": {
                "$path": ".blink/server.luau"
            },
            "server-packages": {
                "$className": "Folder",
                "$path": "./server-packages"
            }
        },
        "StarterPlayer": {
            "$className": "StarterPlayer",
            "$properties": {
                "GameSettingsAvatar": "R6"
            }
        },
        "Workspace": {
            "$path": "./assets/Workspace"
        }
    },
    "syncbackRules": {
        "ignoreTrees": [
            "ReplicatedStorage/client",
            "ReplicatedStorage/common",
            "ReplicatedStorage/packages",
            "ReplicatedStorage/network",
            "ServerScriptService/network",
            "ServerScriptService/server",
            "ServerScriptService/server-packages"
        ],
        "syncUnscriptable": true
    }
}