{
    "name": "chainsaw",
    "emitLegacyScripts": false,
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "client": {
                "$path": "src/client"
            },
            "common": {
                "$path": "src/common"
            },
            "network": {
                "$path": ".blink/client.luau"
            },
            "Packages": {
                "$className": "Folder",
                "$path": "./packages"
            }
        },
        "ServerScriptService": {
            "server": {
                "$path": "src/server"
            },
            "network": {
                "$path": ".blink/server.luau"
            },
            "ServerPackages": {
                "$className": "Folder",
                "$path": "./serverPackages"
            }
        }
    }
}